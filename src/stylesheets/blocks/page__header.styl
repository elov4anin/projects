.container
  width: 100%
  height: 100%
  margin: 0

.wrapper
  margin-left 125px
  width: 715px

.page-header
  margin-top: 45px
  display: flex
  justify-content space-between

.contacts
  display: flex
  flex-direction column

  &__tel
    font-size: 24px;
    text-align: center;
    letter-spacing: -0.02em;
    text-decoration: none

    color: #92A1B0;

    span
      font-weight: 500
      color: #000000;

  &__callback
    font-weight: 500;
    line-height: normal;
    font-size: 14px;
    text-align: center;
    letter-spacing: -0.02em;
    text-decoration-line: underline;

    color: #3079FF;
